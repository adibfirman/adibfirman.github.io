{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "downlevelIteration": true,
    "baseUrl": "./",
    "paths": {
      "@*": ["./*"]
    }
  },
  "exclude": ["node_modules"],
  "include": [
    "env.d.ts",
    "next-env.d.ts",
    "**/*.js",
    "**/*.json",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx"
  ]
}
